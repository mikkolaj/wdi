#include <iostream>
#include <cmath>

using namespace std;
const int N = 10;


int maxPary(int t1[], int t2[]){
	int max = 0;
	int k = pow(2, N);
	
	for(int i = 1; i < k ; i++){
		for(int j = 1 ; j < k ; j++){
			int komb1 = i;
			int komb2 = j;
			int suma1 = 0;
			int suma2 = 0;
			int a = 0;
			int liczPary1 = 0, liczPary2 = 0;
			while(komb1 != 0){
				if(komb1%2 == 1){
					suma1 += t1[a];
					liczPary1++;
				}
				komb1 /= 2;
				a++;
			}
			int b = 0;
			while(komb2 != 0){
				if(komb2%2 == 1){
					suma2 += t2[b];
					liczPary2++;
				}
				komb2 /= 2;
				b++;
			}
			if(suma1 == suma2 && liczPary1 == liczPary2){
				if(liczPary1 > max){
					max = liczPary1;
				}
			}
		}
	}
	
	return max;
}

int main(){
	int t1[5] = {1,4,40,7,700};
	int t2[5] = {3,2,2,58,500};
	cout << maxPary(t1, t2);

	return 0;
}
